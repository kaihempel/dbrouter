    "description": "Dynamic database based routing system",
    "type": "library",
    "keywords": [
        "dynamic content, routing, database"
    ],
    "homepage": "https://www.kuweh.de/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kai Hempel",
            "email": "dev@kuweh.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/kaihempel/dbrouter/issues"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dbrouter\\": "src/"
        }
